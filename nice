cd
mkdir test
cd test
pwd
touch empty.txt
cp /etc/timezone ./
mv timezone tz.txt
ls
cd ..
rm -r test
locate fdisk
find /home -name fdisk
find /home/ -type f -size +200M -exec ls -lh {} \;
find /home/ -iname "*.txt" -mtime -3 -print
find /home/ -iname "*.txt" -atime +30 -print
last -5
tail -n 2 /etc/passwd
grep -r 'sem_post' /usr/include
cut -d ":" -f 1,5 /etc/passwd
cat /etc/motd | tr "[a-z]" "[A-Z]"
cat /dev/random > rand.txt &
jobs
kill -STOP PIDberapa
kill -CONT PIDberapa &
kill -TERM PIDberapa
